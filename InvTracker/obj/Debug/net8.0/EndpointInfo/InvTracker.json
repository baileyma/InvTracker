      "post": {
        "tags": [
          "Update"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentToAdd"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentToAdd"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentToAdd"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Update/update-balance": {
      "put": {
        "tags": [
          "Update"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BalanceToAdd"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/BalanceToAdd"
