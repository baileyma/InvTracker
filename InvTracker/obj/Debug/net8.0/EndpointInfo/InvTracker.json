          "xirr": {
            "type": "number",
            "format": "double"
          },
          "netDeposits": {
            "type": "number",
            "format": "double"
          },
          "totalDeposits": {
            "type": "number",
            "format": "double"
          },
          "totalWithdrawals": {
            "type": "number",
            "format": "double"
          },
          "startBalance": {
            "type": "number",
            "format": "double"
          },
          "endBalance": {
            "type": "number",
            "format": "double"
