          "accountId",
          "accountName",
          "balancesAndReturnsByYear"
        ],
        "type": "object",
        "properties": {
          "accountId": {
            "type": "integer",
            "format": "int32"
          },
          "accountName": {
            "type": "string",
            "nullable": true
          },
          "balancesAndReturnsByYear": {
            "type": "object",
            "additionalProperties": {
              "$ref": "#/components/schemas/BalanceAndXIRR"
            },
            "nullable": true
          },
          "cumulativeReturn": {
            "type": "number",
            "format": "double",
            "readOnly": true
