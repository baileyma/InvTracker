              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AccountYearlyDetail"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/AccountYearlyDetail"
                }
              }
            }
          }
        }
      }
    },
